{
  "expo": {
    "name": "Covy",
    "description": "Tracker application for covid-19",
    "slug": "covy",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "3.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon_512.png",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mediusoft.covy",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.mediusoft.covy",
      "versionCode": 3,
      "permissions": [
        "LOCATION"
      ]
    },
    "githubUrl": "https://github.com/mediusoft/covy"
  }
}
