{
  "expo": {
    "name": "Covy",
    "description": "Tracker application for covid-19",
    "slug": "covy",
    "privacy": "public",
    "version": "1.0.0",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "orientation": "portrait",
    "primaryColor": "#000",
    "icon": "./src/assets/images/icon_512.png",
    "assetBundlePatterns": [
      "src/assets/images/*"
    ],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mediusoft.covy",
      "icon": "./src/assets/images/appstore.png",
      "buildNumber": "1.0.0",
    },
    "android": {
      "package": "com.mediusoft.covy",
      "versionCode": 1,
      "permissions": [
        "ACCESS_FINE_LOCATION",
      ]
    },
    "githubUrl": "https://github.com/mediusoft/covy"
  }
}